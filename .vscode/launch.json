      "name": "COHATCH AfterHours (Full Workflow)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/generate_all.py",
      "args": [
        "--event",
        "cohatch_afterhours"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
