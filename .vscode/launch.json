      "args": [
        "neighborhood_gathering_user_003"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Azure Functions: Start (No Debug)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "bash",
      "runtimeArgs": [
        "-lc",
        "source ${workspaceFolder}/function_app/.venv/bin/activate && func start --port 7072"
      ],
      "cwd": "${workspaceFolder}/function_app",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "console": "integratedTerminal",
      "preLaunchTask": "Kill Port 7072"
    },
    {
      "name": "Azure Functions: Attach",
      "type": "python",
      "request": "attach",
      "port": 9092,
      "host": "localhost",
      "preLaunchTask": "func: host start",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/function_app",
          "remoteRoot": "/azure-functions-host/workers/python"
        }
      ]
    },
    {
      "name": "Test Local Function (Random Attendee)",
